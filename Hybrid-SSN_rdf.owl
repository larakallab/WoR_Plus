    </ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#hasInterfaces -->

    <ObjectProperty rdf:about="&hssn;hasInterfaces">
        <rdfs:label>hasInterfaces</rdfs:label>
        <skos:definition>This relation maps a Service to its Interfaces.</skos:definition>
        <rdfs:range rdf:resource="&hssn;Interface"/>
        <rdfs:domain rdf:resource="&hssn;Service"/>
    </ObjectProperty>
    


