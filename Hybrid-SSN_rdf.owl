    <ObjectProperty rdf:about="&hssn;hasMetadata">
        <rdfs:label>hasMetadata</rdfs:label>
        <skos:definition>This relation maps a Thing to its Metadata descriptors.</skos:definition>
        <rdfs:range rdf:resource="&hssn;Metadata"/>
        <rdfs:domain rdf:resource="&owl;Thing"/>
    </ObjectProperty>
    


