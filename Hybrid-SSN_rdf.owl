    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2004/02/skos/core#note -->

    <AnnotationProperty rdf:about="&skos;note"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://mssn.sigappfr.org/mssn#above -->

    <ObjectProperty rdf:about="&mssn;above">
        <rdfs:label>isAbove</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&mssn;directionalRelation"/>
        <rdfs:domain rdf:resource="&hssn;Location"/>
        <rdfs:range rdf:resource="&hssn;Location"/>
    </ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn#below -->

    <ObjectProperty rdf:about="&mssn;below">
        <rdfs:label>isBelow</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&mssn;directionalRelation"/>
        <rdfs:range rdf:resource="&hssn;Location"/>
        <rdfs:domain rdf:resource="&hssn;Location"/>
    </ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn#closeTo -->

    <ObjectProperty rdf:about="&mssn;closeTo">
        <rdfs:label>isCloseTo</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&mssn;distanceRelation"/>
        <rdfs:range rdf:resource="&hssn;Location"/>
        <rdfs:domain rdf:resource="&hssn;Location"/>
    </ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn#contains -->

    <ObjectProperty rdf:about="&mssn;contains">
        <rdfs:label>contains</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&mssn;topologicalRelation"/>
        <rdfs:domain rdf:resource="&hssn;Location"/>
        <rdfs:range rdf:resource="&hssn;Location"/>
    </ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn#covers -->

    <ObjectProperty rdf:about="&mssn;covers">
        <rdfs:label>covers</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&mssn;topologicalRelation"/>
        <rdfs:domain rdf:resource="&hssn;Location"/>
        <rdfs:range rdf:resource="&hssn;Location"/>
    </ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn#crosses -->

    <ObjectProperty rdf:about="&mssn;crosses">
        <rdfs:label>crosses</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&mssn;topologicalRelation"/>
        <rdfs:domain rdf:resource="&hssn;Location"/>
        <rdfs:range rdf:resource="&hssn;Location"/>
    </ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn#directionalRelation -->

    <ObjectProperty rdf:about="&mssn;directionalRelation">
        <rdfs:label>hasDirectionalLocation</rdfs:label>
        <vann:preferredNamespaceUri>An abstract relation for directional location relations</vann:preferredNamespaceUri>
        <rdfs:subPropertyOf rdf:resource="&mssn;locationRelation"/>
        <rdfs:domain rdf:resource="&hssn;Location"/>
        <rdfs:range rdf:resource="&hssn;Location"/>
    </ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn#disjoint -->

    <ObjectProperty rdf:about="&mssn;disjoint">
        <rdfs:label>isDisjointWith</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&mssn;topologicalRelation"/>
        <rdfs:domain rdf:resource="&hssn;Location"/>
        <rdfs:range rdf:resource="&hssn;Location"/>
    </ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn#distanceRelation -->

    <ObjectProperty rdf:about="&mssn;distanceRelation">
        <rdfs:label>hasDistanceRelation</rdfs:label>
        <vann:preferredNamespaceUri>An abstract relation for distance relations</vann:preferredNamespaceUri>
        <rdfs:subPropertyOf rdf:resource="&mssn;locationRelation"/>
        <rdfs:domain rdf:resource="&hssn;Location"/>
        <rdfs:range rdf:resource="&hssn;Location"/>
    </ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn#equals -->

    <ObjectProperty rdf:about="&mssn;equals">
        <rdfs:label>equals</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&mssn;topologicalRelation"/>
        <rdfs:range rdf:resource="&hssn;Location"/>
        <rdfs:domain rdf:resource="&hssn;Location"/>
    </ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn#farFrom -->

    <ObjectProperty rdf:about="&mssn;farFrom">
        <rdfs:label>isFarFrom</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&mssn;distanceRelation"/>
        <rdfs:range rdf:resource="&hssn;Location"/>
        <rdfs:domain rdf:resource="&hssn;Location"/>
    </ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn#hasMediaDescriptor -->

    <ObjectProperty rdf:about="&mssn;hasMediaDescriptor">
        <rdfs:label>hasMultimediaDescriptor</rdfs:label>
        <rdfs:comment>A relation between a media segment and a media descriptor</rdfs:comment>
        <rdfs:range rdf:resource="&mssn;MediaDescriptor"/>
        <rdfs:domain rdf:resource="&mssn;MultimediaData"/>
    </ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn#hasMediaSegment -->

    <ObjectProperty rdf:about="&mssn;hasMediaSegment">
        <rdfs:label>hasMediaSegment</rdfs:label>
        <rdfs:comment>A relation between MultimediaData and MediaSegment concept</rdfs:comment>
        <rdfs:domain rdf:resource="&mssn;MediaDescriptor"/>
        <rdfs:range rdf:resource="&mssn;MultimediaData"/>
    </ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn#leftOf -->

    <ObjectProperty rdf:about="&mssn;leftOf">
        <rdfs:label>isLeftOf</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&mssn;directionalRelation"/>
        <rdfs:domain rdf:resource="&hssn;Location"/>
        <rdfs:range rdf:resource="&hssn;Location"/>
    </ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn#locationRelation -->

    <ObjectProperty rdf:about="&mssn;locationRelation">
        <rdfs:label>hasLocationRelation</rdfs:label>
        <rdfs:comment>A location relation describes a relation between a pair of locations e.g. &quot;Table1&quot; opposite &quot;Table2&quot;.   

A location relation can be either topological relation, directional relation or distance relation.

This relation intends to be use as an abstract relation for other location relations.   Hence, it may not be used directly.    Only sub-relations of this relation should be used</rdfs:comment>
        <rdfs:range rdf:resource="&hssn;Location"/>
        <rdfs:domain rdf:resource="&hssn;Location"/>
    </ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn#opposite -->

    <ObjectProperty rdf:about="&mssn;opposite">
        <rdfs:label>isOppositeOf</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&mssn;directionalRelation"/>
        <rdfs:domain rdf:resource="&hssn;Location"/>
        <rdfs:range rdf:resource="&hssn;Location"/>
    </ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn#overlaps -->

    <ObjectProperty rdf:about="&mssn;overlaps">
        <rdfs:label>overlaps</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&mssn;topologicalRelation"/>
        <rdfs:range rdf:resource="&hssn;Location"/>
        <rdfs:domain rdf:resource="&hssn;Location"/>
    </ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn#rightOf -->

    <ObjectProperty rdf:about="&mssn;rightOf">
        <rdfs:label>isRightOf</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&mssn;directionalRelation"/>
        <rdfs:range rdf:resource="&hssn;Location"/>
        <rdfs:domain rdf:resource="&hssn;Location"/>
    </ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn#topologicalRelation -->

    <ObjectProperty rdf:about="&mssn;topologicalRelation">
        <rdfs:label>hasTopologicalRelation</rdfs:label>
        <vann:preferredNamespaceUri>An abstract relation for topological location relations</vann:preferredNamespaceUri>
        <rdfs:subPropertyOf rdf:resource="&mssn;locationRelation"/>
        <rdfs:range rdf:resource="&hssn;Location"/>
        <rdfs:domain rdf:resource="&hssn;Location"/>
    </ObjectProperty>
    


    <!-- http://mssn.sigappfr.org/mssn#touches -->

    <ObjectProperty rdf:about="&mssn;touches">
        <rdfs:label>touches</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="&mssn;topologicalRelation"/>
        <rdfs:range rdf:resource="&hssn;Location"/>
        <rdfs:domain rdf:resource="&hssn;Location"/>
    </ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#affectedBy -->

    <ObjectProperty rdf:about="&hssn;affectedBy">
        <rdfs:label>affectedBy</rdfs:label>
        <skos:definition>Relation that maps a Visualisation to a Context</skos:definition>
        <rdfs:range rdf:resource="&hssn;Context"/>
        <rdfs:domain rdf:resource="&hssn;Visualisation"/>
    </ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#affects -->

    <ObjectProperty rdf:about="&hssn;affects">
        <rdfs:label>affects</rdfs:label>
        <skos:definition>Relation that maps a Context to a Visualisation choice</skos:definition>
        <rdfs:domain rdf:resource="&hssn;Context"/>
        <rdfs:range rdf:resource="&hssn;Visualisation"/>
        <inverseOf rdf:resource="&hssn;affectedBy"/>
    </ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#atCoverageTime -->

    <ObjectProperty rdf:about="&hssn;atCoverageTime">
        <rdfs:label>hasCoverageTime</rdfs:label>
        <skos:definition>Relation that maps a CoverageArea covered by a Sensor to a time:TemporalEntity.</skos:definition>
        <rdfs:domain rdf:resource="&hssn;CoverageArea"/>
        <rdfs:range rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
    </ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#atLocationTime -->

    <ObjectProperty rdf:about="&hssn;atLocationTime">
        <rdfs:label>hasLocationTime</rdfs:label>
        <skos:definition>Relation that maps a Location of a Sensor to a time:TemporalEntity.</skos:definition>
        <rdfs:domain rdf:resource="&hssn;Location"/>
        <rdfs:range rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
    </ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#atSensingTime -->

    <ObjectProperty rdf:about="&hssn;atSensingTime">
        <rdfs:label>hasSensingTime</rdfs:label>
        <rdfs:comment>Denotes the sensing timestamp for a specific Observation Value</rdfs:comment>
        <rdfs:domain rdf:resource="&hssn;ObservationValue"/>
        <rdfs:range rdf:resource="http://www.w3.org/2006/time#TemporalEntity"/>
    </ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#centeredAround -->

    <ObjectProperty rdf:about="&hssn;centeredAround">
        <rdfs:label>isCenteredAround</rdfs:label>
        <skos:definition>Relation that maps a Sensor&apos;s Location to its CoverageArea.</skos:definition>
        <rdfs:domain rdf:resource="&hssn;CoverageArea"/>
        <rdfs:range rdf:resource="&hssn;SensingLocation"/>
    </ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#centers -->

    <ObjectProperty rdf:about="&hssn;centers">
        <rdfs:label>centers</rdfs:label>
        <skos:definition>Relation that maps a CoverageArea to its Sensor&apos;s Location.</skos:definition>
        <rdfs:range rdf:resource="&hssn;CoverageArea"/>
        <rdfs:domain rdf:resource="&hssn;SensingLocation"/>
        <inverseOf rdf:resource="&hssn;centeredAround"/>
    </ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#composedOf -->

    <ObjectProperty rdf:about="&hssn;composedOf">
        <rdfs:label>isComposedOf</rdfs:label>
        <skos:definition>The relation between a LocationMap and the set of individual Locations that compose it.</skos:definition>
        <rdfs:range rdf:resource="&hssn;Location"/>
        <rdfs:domain rdf:resource="&hssn;LocationMap"/>
        <inverseOf rdf:resource="&hssn;composes"/>
    </ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#composes -->

    <ObjectProperty rdf:about="&hssn;composes">
        <rdfs:label>composes</rdfs:label>
        <skos:definition>The relation between the set of individual Locations that compose a LocationMap.</skos:definition>
        <rdfs:domain rdf:resource="&hssn;Location"/>
        <rdfs:range rdf:resource="&hssn;LocationMap"/>
    </ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#currentlyCoveredBy -->

    <ObjectProperty rdf:about="&hssn;currentlyCoveredBy">
        <rdfs:label>isCurrentlyCoveredBy</rdfs:label>
        <rdfs:comment>The inverse of currentlyCoversA</rdfs:comment>
        <skos:definition>The inverse of currentlyCoversA</skos:definition>
        <rdfs:domain rdf:resource="&hssn;CoverageArea"/>
        <inverseOf rdf:resource="&hssn;currentlyCoversA"/>
        <rdfs:range rdf:resource="&sosa;Sensor"/>
    </ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#currentlyCoversA -->

    <ObjectProperty rdf:about="&hssn;currentlyCoversA">
        <rdfs:label>currentlyCovers</rdfs:label>
        <skos:definition>currentlyCoversA is the object property that allows finding the Coverage Area of a Stationary or Mobile Sensor in time. The Coverage Area of a Stationary Sensor does not vary in time, this is not the case for a Mobile Sensor. At a point t in time, a Stationary or Mobile Sensor can have exactly one Coverage Area.</skos:definition>
        <rdfs:comment>currentlyCoversA is the object property that allows finding the Coverage Area of a Stationary or Mobile Sensor in time. The Coverage Area of a Stationary Sensor does not change in time, this is not the case for a Mobile Sensor. At a point t in time, a Stationary or Mobile Sensor can have exactly one Coverage Area.</rdfs:comment>
        <rdfs:range rdf:resource="&hssn;CoverageArea"/>
        <rdfs:domain rdf:resource="&sosa;Sensor"/>
    </ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#currentlyLocatedAt -->

    <ObjectProperty rdf:about="&hssn;currentlyLocatedAt">
        <rdfs:label>isCurrentlyLocatedAt</rdfs:label>
        <skos:definition>currentlyLocatedAt is the object property that allows to localize a Stationary or Mobile Sensor in time. The  Location value for a Stationary Sensor does not change in time, this is not the case for a Mobile Sensor. At a point t in time, a Stationary or Mobile Sensor can have exactly one Location.</skos:definition>
        <rdfs:comment>curentlyLocatedAt is the object property that allows to localize a Stationary or Mobile Sensor in time. The Location value for a Stationary Sensor does not change in time, this is not the case for a Moble Sensor. At a point t in time, a Stationary or Mobile Sensor can have exactly one Location.</rdfs:comment>
        <rdfs:domain rdf:resource="&hssn;Device"/>
        <rdfs:range rdf:resource="&hssn;Location"/>
        <inverseOf rdf:resource="&hssn;currentlyLocates"/>
    </ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#currentlyLocates -->

    <ObjectProperty rdf:about="&hssn;currentlyLocates">
        <rdfs:label>currentlyLocates</rdfs:label>
        <rdfs:comment>The inverse of currentlyLocatedAt</rdfs:comment>
        <skos:definition>The inverse of currentlyLocatedAt</skos:definition>
        <rdfs:range rdf:resource="&hssn;Device"/>
        <rdfs:domain rdf:resource="&hssn;Location"/>
    </ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#delimitedBy -->

    <ObjectProperty rdf:about="&hssn;delimitedBy">
        <rdfs:label>isDelimitedBy</rdfs:label>
        <skos:definition>Defines the relation between CoverageArea and the sensor&apos;s SensingRange.</skos:definition>
        <rdfs:domain rdf:resource="&hssn;CoverageArea"/>
        <rdfs:range rdf:resource="&hssn;SensingRange"/>
    </ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#delimits -->

    <ObjectProperty rdf:about="&hssn;delimits">
        <rdfs:label>delimits</rdfs:label>
        <skos:definition>Defines the impact of the sensor&apos;s SensingRange on the geomerty of the CoverageArea.</skos:definition>
        <rdfs:range rdf:resource="&hssn;CoverageArea"/>
        <rdfs:domain rdf:resource="&hssn;SensingRange"/>
        <inverseOf rdf:resource="&hssn;delimitedBy"/>
    </ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#describes -->

    <ObjectProperty rdf:about="&hssn;describes">
        <rdfs:label>describes</rdfs:label>
        <skos:definition>The relation between a LocationMap and an Infrastructure helps represent the spatial elements of the latter.</skos:definition>
        <rdfs:range rdf:resource="&hssn;Infrastructure"/>
        <rdfs:domain rdf:resource="&hssn;LocationMap"/>
    </ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#embeddedOn -->

    <ObjectProperty rdf:about="&hssn;embeddedOn">
        <rdfs:label>isEmbeddedOn</rdfs:label>
        <skos:definition>The relation helps discover the Systems deployed or embedded on a Device&apos;s ExpansionCard.</skos:definition>
        <rdfs:range rdf:resource="&hssn;ExpansionCard"/>
        <inverseOf rdf:resource="&hssn;hasEmbeddedSystem"/>
        <rdfs:domain rdf:resource="&ssn;System"/>
    </ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#hasCapability -->

    <ObjectProperty rdf:about="&hssn;hasCapability">
        <rdfs:label>hasCapability</rdfs:label>
        <skos:definition>This relation maps a Service to its Capability description.</skos:definition>
        <rdfs:range rdf:resource="&hssn;Capability"/>
        <rdfs:domain rdf:resource="&hssn;Service"/>
    </ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#hasComUnit -->

    <ObjectProperty rdf:about="&hssn;hasComUnit">
        <rdfs:label>hasNetworkInterface</rdfs:label>
        <skos:definition>This relation maps the Hardware of a Device to its NetworkInterface.</skos:definition>
        <rdfs:domain rdf:resource="&hssn;Hardware"/>
        <rdfs:range rdf:resource="&hssn;NetworkInterface"/>
    </ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#hasCommunicationRange -->

    <ObjectProperty rdf:about="&hssn;hasCommunicationRange">
        <rdfs:range rdf:resource="&hssn;CommunicationRange"/>
        <rdfs:domain rdf:resource="&hssn;NetworkInterface"/>
    </ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#hasContext -->

    <ObjectProperty rdf:about="&hssn;hasContext">
        <rdfs:label>hasCurrentContext</rdfs:label>
        <skos:definition>Relation that maps a Thing to its current Context</skos:definition>
        <rdfs:range rdf:resource="&hssn;Context"/>
        <rdfs:domain rdf:resource="&owl;Thing"/>
    </ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#hasDeployUnit -->

    <ObjectProperty rdf:about="&hssn;hasDeployUnit">
        <rdfs:label>hasExpansionCard</rdfs:label>
        <skos:definition>This relation maps the Hardware of a Device to its ExpansionCard.</skos:definition>
        <rdfs:range rdf:resource="&hssn;ExpansionCard"/>
        <rdfs:domain rdf:resource="&hssn;Hardware"/>
        <inverseOf rdf:resource="&hssn;unitDeployedOn"/>
    </ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#hasEmbeddedSystem -->

    <ObjectProperty rdf:about="&hssn;hasEmbeddedSystem">
        <rdfs:label>hasEmbeddedSystem</rdfs:label>
        <skos:definition>This relation maps Systems to ExpansionCards where they are embedded or deployed.</skos:definition>
        <rdfs:domain rdf:resource="&hssn;ExpansionCard"/>
        <rdfs:range rdf:resource="&ssn;System"/>
    </ObjectProperty>
    


    <!-- http://spider.sigappfr.org/HSSNdoc/hssn.ttl#hasHardware -->

    <ObjectProperty rdf:about="&hssn;hasHardware">
        <rdfs:label>hasHardware</rdfs:label>
        <skos:definition>This relation maps a Device to its Hardware.</skos:definition>
        <rdfs:domain rdf:resource="&hssn;Device"/>
        <rdfs:range rdf:resource="&hssn;Hardware"/>
